{
  "name": "@itleadopencommerce/reaction-marketplace",
  "description": "A multi-vendor marketplace plugin for Reaction Commerce",
  "version": "0.2.2",
  "main": "index.js",
  "type": "module",
  "homepage": "https://github.com/it-lead-team/reaction-marketplace.git",
  "url": "https://github.com/it-lead-team/reaction-marketplace.git",
  "email": "loan@outgrow.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/it-lead-team/reaction-marketplace.git"
  },
  "author": {
    "name": "out:grow & Oleh Obodin",
    "email": "dev@it-lead.eu",
    "url": "https://it-lead.eu"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/it-lead-team/reaction-marketplace.git/issues"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "exports": {
    ".": "./lib/",
    "./graphql/": "./lib/graphql/",
    "./tests/": "./lib/tests/"
  },
  "sideEffects": false,
  "dependencies": {
    "@reactioncommerce/api-utils": "^1.7.2",
    "@reactioncommerce/logger": "^1.1.3",
    "@reactioncommerce/random": "^1.0.2",
    "@reactioncommerce/reaction-error": "^1.0.1",
    "envalid": "^6.0.1",
    "graphql": "^14.5.8",
    "graphql-fields": "^2.0.3",
    "graphql-relay": "^0.6.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@reactioncommerce/eslint-config": "^2.1.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-rewire-exports": "^2.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "jest": "^24.9.0",
    "semantic-release": "^15.13.24"
  },
  "scripts": {
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:file": "jest --no-cache --watch --coverage=false"
  },
  "babel": {
    "plugins": [
      "rewire-exports",
      "transform-es2015-modules-commonjs"
    ]
  },
  "eslintConfig": {
    "extends": "@reactioncommerce",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true
      }
    },
    "env": {
      "es6": true,
      "jasmine": true
    }
  },
  "prettier": {
    "arrowParens": "always"
  },
  "publishConfig": {
    "access": "public"
  }
}
